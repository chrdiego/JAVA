package Ejercicios;

public class Resta {

	private double punto = 0;
	private double pendiente = 0;
	
	public Resta(double punto, double pendiente) {
		this.punto = punto;
		this.pendiente = pendiente;
	}
	
	public boolean esPunto(Resta r) {
		return true;
	}
	
	public String toString() {
		return "(" + punto + ", " + pendiente + ")";
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	Resta r = new Resta(5, 4/5);
	r.toString();
	}

}
